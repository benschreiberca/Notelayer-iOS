match_git_url = ENV["MATCH_GIT_URL"].to_s.strip
git_url(match_git_url) unless match_git_url.empty?

match_git_branch = ENV["MATCH_GIT_BRANCH"].to_s.strip
git_branch(match_git_branch) unless match_git_branch.empty?
storage_mode("git")
type("appstore")
app_identifier(["com.notelayer.app"])
readonly(ENV["CI"] == "true")
